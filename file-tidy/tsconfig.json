{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "module": "esnext",
    "target": "es2015",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "typeRoots": [
      "./node_modules/@types",
    ],
    "lib": [
      "esnext",
      "dom"
    ],
    "jsx": "preserve",
    "noUnusedLocals": false,
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  }
}
